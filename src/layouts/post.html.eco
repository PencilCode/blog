---
layout: default
---
<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <%- @partial('ppost', {title: @document.title, url: @document.url, tags: @document.tags, date: @document.date, content: @content, individualPost: true }) %>
            <%- @partial('livefyre') if @document.comments %>
            <%- @partial('us', false) %>
        </div>
    </div>
</div>

