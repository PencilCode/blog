{
  "name": "pencilcode-blog",
  "version": "0.0.1",
  "description": "Source for blog.pencilcode.net",
  "author": "David Bau <david.bau@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/PencilCode/blog.git"
  },
  "license": "MIT",
  "dependencies": {
    "docpad": "~6.78",
    "cheerio": "latest",
    "moment": "latest",
    "docpad-plugin-marked": ">=2.2",
