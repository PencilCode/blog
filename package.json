    "docpad-plugin-less": ">=2.4",
    "docpad-plugin-coffeescript": ">=2.4",
    "docpad-plugin-sitemap": ">=2.2",
    "docpad-plugin-partials": ">=2.9",
    "docpad-plugin-cleanurls": ">=2.7",
    "docpad-plugin-tags": ">=2.0",
    "docpad-plugin-dateurls": ">=2.4",
    "docpad-plugin-paged": "https://github.com/ervwalter/docpad-plugin-paged/tarball/compiled",
    "docpad-plugin-livereload": ">=2.6"
  },
  "devDependencies": {},
  "engines": {
    "node": "0.10",
    "npm": "1.4"
  },
  "main": "./node_modules/docpad/bin/docpad-server",
  "scripts": {
    "start": "node ./node_modules/docpad/bin/docpad-server",
